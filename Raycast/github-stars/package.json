{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-stars",
  "title": "Github Stars",
  "description": "Count total number of stars of a Github user.",
  "icon": "command-icon.png",
  "author": "Pondorasti",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Github Stars",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
