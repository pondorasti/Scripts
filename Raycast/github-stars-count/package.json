{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-stars-count",
  "title": "GitHub Stars Count",
  "description": "Count total number of stars of a GitHub user.",
  "icon": "icon.png",
  "author": "Pondorasti",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Stars Count",
      "subtitle": "GitHub",
